render_mode unshaded;

uniform vec4 color : hint_color;
uniform float frequency = 300;
uniform float depth = 0.0005;

void fragment()
{
	//COLOR = color; 
	vec2 uv = UV;
	float freq = max(frequency, sin(uv.x + TIME) * 500.0);
	float dep = max(depth, cos(uv.y + TIME) * 0.01);

	uv.x += sin(uv.y * freq + TIME) * dep;
	uv.x = clamp(uv.x, 0.0, 1.0);
	COLOR.rgba = texture(TEXTURE, uv);

}"

